{
    "name": "beatwiz/statamic-entry-export-pdf",
    "description": "Export Statamic Entries as PDF",
    "license": "MIT",
    "require": {
        "statamic/cms": "^5.0.0",
        "barryvdh/laravel-dompdf": "^2.0.0"
    },
    "autoload": {
        "psr-4": {
            "Beatwiz\\StatamicEntryExportPdf\\": "src"
        }
    },
    "authors": [
        {
            "name": "Gustavo Silva"
        }
    ],
    "support": {
        "email": "dev@provider.pt"
    },
    "extra": {
        "statamic": {
            "name": "Export Entry as PDF",
            "description": "Export Statamic Entries as PDF",
            "editions": ["free"]
        },
        "laravel": {
            "providers": [
                "Beatwiz\\StatamicEntryExportPdf\\ServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "pixelfear/composer-dist-plugin": true
        }
    }
}
